<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>Title</title>
</head>
<body>

<!-- Begin Page Content -->
<div class="container-fluid">
    <!--Intput table card-->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">International Card Issuance</h6>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-lg-10">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <form id="bankDeposit" method="POST" action="<c:url value='/save'/>"
                                          enctype="multipart/form-data">

                                        <div class="form-group row">
                                            <label class="align-right required col-lg-2"
                                                   for="cid">Search By CID</label>

                                            <div class="col-lg-5">
                                                <input type="number" name="cid" id="cid"
                                                       class="form-control-file" required>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <input type="reset" id="btnReset" class="btn btn-primary"
                                                   value="Reset">

                                            <%--<security:authorize access="hasRole('${SCREEN_ID}-ADD')">--%>
                                            <input type="submit" id="btnSave" value="Save"
                                                   class="btn btn-primary  editable">
                                            <%--</security:authorize>--%>
                                        </div>
                                    </form>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->

                <div class="col-lg-2" id="db_excel_format"></div>
            </div>
        </div>
    </div>
</div>

</body>
</html>
